{
  "name": "hovercraft",
  "displayName": "`hovercraft` - CSV/JSON-Powered Hover Provider for VSCode",
  "description": "Provides hover information from CSV/JSON files in your workspace",
  "version": "0.2.1",
  "publisher": "kdheepak",
  "repository": {
    "type": "git",
    "url": "https://github.com/kdheepak/vscode-hovercraft"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Hovercraft",
      "properties": {
        "hovercraft.logLevel": {
          "type": "string",
          "default": "info",
          "enum": [
            "debug",
            "info",
            "warning",
            "error"
          ],
          "description": "Log level for the language server"
        },
        "hovercraft.uvPath": {
          "type": "string",
          "default": "",
          "description": "Path to the uv executable. Leave blank to use the default uv in your system PATH."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "glob": "^11.0.0",
    "vscode-languageclient": "^9.0.1"
  }
}
